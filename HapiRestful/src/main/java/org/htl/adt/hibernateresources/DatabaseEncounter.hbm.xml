<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.htl.adt.hibernateresources">
    <class name="DatabaseEncounter" table="Encounter">
        
        <id name="encounterIdentifier" column="Identifier"/>
        
        <property name="encounter_id" column="Encounter_id"/>     
        <property name="patientIdentifier" column="PatientIdentifier"/>
      	<property name="priority" column="Priority"/>
      	<property name="reason" column="Reason"/>           	
      	<property name="status" column="Status"/>
      	<property name="fhirMessage" column="FhirMessage"/>
      	
      	
    </class>
    
    
</hibernate-mapping>