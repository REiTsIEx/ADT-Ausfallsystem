<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.htl.adt.hibernateresources">
    <class name="DatabaseLocation" table="Location">
        
        <id name="locationIdentifier" column="Identifier"/>
        
        <property name="location_id" column="Location_id"/>     
      	<property name="name" column="Name"/>
      	<property name="status" column="Status"/>           	
      	<property name="description" column="Description"/>
      	<property name="fhirMessage" column="FhirMessage"/>
      	
      	
    </class>
    
    
</hibernate-mapping>