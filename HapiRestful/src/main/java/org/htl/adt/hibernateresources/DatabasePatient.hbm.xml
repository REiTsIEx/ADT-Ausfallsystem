<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.htl.adt.hibernateresources">
    <class name="DatabasePatient" table="Patient">
        
        <id name="patientIdentifier" column="Identifier"/>
        
        <property name="patient_id" column="Patient_id"/>     
        <property name="firstName" column="FirstName"/>
      	<property name="familyName" column="LastName"/>
      	<property name="patientGender" column="Gender"/>           	
      	<property name="fhirMessage" column="FhirMessage"/>
      	
    </class>
    
    
</hibernate-mapping>