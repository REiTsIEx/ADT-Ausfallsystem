<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<Configuration status="warn">
	<Appenders>
		<Console name="console" target="SYSTEM_OUT">
			<PatternLayout pattern="%d %-5p [%t] %C{2} (%F:%L) - %m%n" />
		</Console>
		
	</Appenders>
	
	<Loggers>
		<Root level="all">
			<AppenderRef ref="console" level="all"/>
		</Root>
	</Loggers>
</Configuration>